<%- include('../partials/header') %>
<table class="box" id="list">
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>Flight #</th>
      <th>Departure (Date/Time)</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(f => { %>
    <tr>
      <td><%= f.airline %></td>
      <td><%= f.airport %></td>
      <td><%= f.flightNo %></td>
      <td><%= f.departs %></td>
      <td><a href="/flights">MAYBE A LINK HERE</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>
